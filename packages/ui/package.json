{
    "name": "@turbo/ui",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "source": "./src/index.ts",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "README.md"
    ],
    "sideEffects": false,
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "type-check": "tsc --noEmit",
        "lint": "eslint . --max-warnings 0",
        "preview": "vite preview",
        "generate:component": "turbo gen react-component"
    },
    "devDependencies": {
        "@turbo/eslint-config": "workspace:*",
        "@turbo/gen": "^1.12.4",
        "@turbo/typescript-config": "workspace:*",
        "@types/eslint": "^8.56.5",
        "@types/node": "^20.11.24",
        "@vitejs/plugin-react-swc": "^3.7.0",
        "eslint": "^8.57.0",
        "typescript": "^5.3.3",
        "vite": "^5.4.0",
        "vite-plugin-dts": "^4.0.3"
    },
    "peerDependencies": {
        "@types/react": "*",
        "@types/react-dom": "*",
        "react": "^18.0 || ^19.0 || ^19.0.0-rc",
        "react-dom": "^18.0 || ^19.0 || ^19.0.0-rc"
    },
    "peerDependenciesMeta": {
        "@types/react": {
            "optional": true
        },
        "@types/react-dom": {
            "optional": true
        }
    }
}
