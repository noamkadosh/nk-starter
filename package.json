{
  "name": "nk",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "turbo build --continue",
    "dev": "turbo dev",
    "type-check": "turbo type-check --continue",
    "lint": "turbo lint --continue",
    "format": "prettier --write .",
    "prepare": "husky",
    "generate:resume": "turbo generate:resume --filter=\"@nk/resume\""
  },
  "devDependencies": {
    "@nk/eslint-config": "workspace:*",
    "@nk/typescript-config": "workspace:*",
    "browserslist": "^4.24.2",
    "concurrently": "^9.1.0",
    "eslint": "^9.17.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.4.0",
  "engines": {
    "node": "20.x",
    "pnpm": "9.x"
  },
  "volta": {
    "node": "20.15.0"
  }
}
